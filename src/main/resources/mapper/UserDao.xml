<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.github.springwind.modules.dao.UserDao">

    <insert id="addUser" parameterType="com.github.springwind.modules.entity.UserInfo">
        insert
        user(user_id, age, name) values (#{userId}, #{age}, #{name})
    </insert>

    <select id="getUser" parameterType="java.lang.String" resultType="java.lang.String">
        select user_id, name, age from userInfo where user_id = #{userId}
    </select>
</mapper>